    <sys:String x:Key="OptionsView:UninstallAllModsButton">Uninstall All Mods</sys:String>
    <sys:String x:Key="OptionsView:OneClickHandlerLabel">OneClick™ Support</sys:String>
    <sys:String x:Key="OptionsView:BeatSaverLabel">BeatSaver</sys:String>
    <sys:String x:Key="OptionsView:ModelSaberLabel">ModelSaber</sys:String>
    <sys:String x:Key="OptionsView:PlaylistLabel">Playlist</sys:String>
