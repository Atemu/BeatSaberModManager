    <sys:String x:Key="OptionsView:MiscLabel">Misc</sys:String>
    <sys:String x:Key="OptionsView:ThemesLabel">Themes</sys:String>
    <sys:String x:Key="OptionsView:ToolsLabel">Tools</sys:String>
    <sys:String x:Key="OptionsView:InstallPlaylistButton">Install Playlist</sys:String>

    <sys:String x:Key="InstallDirDialogWindow:NoInstallDirText">It looks like you haven't set your installation directory yet, would you like to do so?</sys:String>
    <sys:String x:Key="InstallDirDialogWindow:ContinueButtonText">Continue</sys:String>
    <sys:String x:Key="InstallDirDialogWindow:CancelButtonText">Cancel</sys:String>

