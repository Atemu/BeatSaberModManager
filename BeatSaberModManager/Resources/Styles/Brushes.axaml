<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:CompileBindings="True">

    <Styles.Resources>
        <Color x:Key="SystemErrorColor">#FF8B0000</Color>
        <SolidColorBrush x:Key="SystemErrorBrush" Color="{StaticResource SystemErrorColor}" />
        <SolidColorBrush x:Key="BackgroundMediumLowBrush" Color="{DynamicResource SystemChromeMediumLowColor}" Opacity="0.8" />
        <SolidColorBrush x:Key="BackgroundMediumBrush" Color="{DynamicResource SystemChromeMediumColor}" Opacity="0.9675" />
        <SolidColorBrush x:Key="BackgroundHighBrush" Color="{DynamicResource SystemChromeBlackMediumLowColor}" />
    </Styles.Resources>

</Styles>
