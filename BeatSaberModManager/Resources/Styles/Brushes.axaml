<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Styles.Resources>
        <SolidColorBrush x:Key="BackgroundMediumLowBrush" Color="{DynamicResource SystemChromeMediumLowColor}" Opacity="0.9" />
        <SolidColorBrush x:Key="BackgroundMediumBrush" Color="{DynamicResource SystemChromeMediumColor}" Opacity="0.975" />
        <SolidColorBrush x:Key="InfoIconBrush" Color="{DynamicResource SystemChromeAltLowColor}" />
        <SolidColorBrush x:Key="DatabaseIconBrush" Color="{DynamicResource SystemChromeAltLowColor}" />
        <SolidColorBrush x:Key="GearIconBrush" Color="{DynamicResource SystemChromeAltLowColor}" />
    </Styles.Resources>
    
</Styles>